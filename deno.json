{
  "lock": false,
  "tasks": {
    "start": "deno run --unstable -A --watch=static/,routes/ dev.ts",
    "update": "deno run -A -r https://fresh.deno.dev/update .",
    "build": "deno run --unstable -A dev.ts build",
    "preview": "deno run --unstable -A main.ts",
    "test": "DATABASE_PATH=./tmp/test/kv/asset-catalog STORAGE_PATH=./tmp/test/storage deno test --unstable -A"
  },
  "fmt": {
    "exclude": [
      "_fresh"
    ]
  },
  "lint": {
    "include": [
      "./**/*.ts",
      "./**/*.tsx"
    ],
    "exclude": [
      "_fresh"
    ],
    "rules": {
      "tags": [
        "fresh",
        "recommended"
      ]
    }
  },
  "imports": {
    "$fresh/": "https://deno.land/x/fresh@1.4.3/",
    "preact": "https://esm.sh/preact@10.15.1",
    "preact/": "https://esm.sh/preact@10.15.1/",
    "preact/compat": "https://esm.sh/preact@10.16.0/compat",
    "preact-render-to-string": "https://esm.sh/*preact-render-to-string@6.2.1",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.1.3",
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.2.3",
    "$std/": "https://deno.land/std@0.193.0/",
    "$/": "./",
    "cliffy/": "https://deno.land/x/cliffy@v1.0.0-rc.3/",
    "ulid/": "https://deno.land/x/ulid@v0.3.0/",
    "icons/": "https://deno.land/x/tabler_icons_tsx@0.0.3/tsx/"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}
